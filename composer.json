{
  "name": "pdazcom/laravel-walletone",
  "description": "Laravel integration walletone.com",
  "homepage": "http://github.com/pdazcom/laravel-walletone",
  "license": "MIT",
  "authors": [
    {
      "name": "Konstantin A.",
      "email": "kostya.dn@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
